      "explanation_video": null,
      "explanation_images": [],
      "explanation_audio_eng": "https://image.prepladder.com/content/NNs3R42nILh0k2BdS0ZP1742807172.mp3",
      "explanation_audio_hin": "https://image.prepladder.com/content/T3kDr5rqXaTfieJsUtFC1742807220.mp3"
