        7
      ],
      "solution": "<p><span style=\"font-size:12pt\"><span style=\"font-family:Calibri,sans-serif\"><strong>Ans. D. Rabies</strong></span></span></p>\r\n\r\n<p>&nbsp;</p>\r\n\r\n<p><span style=\"font-size:12pt\"><span style=\"font-family:Calibri,sans-serif\"><strong>Option D:</strong></span></span></p>\r\n\r\n<p>&nbsp;</p>\r\n\r\n<p><span style=\"font-size:12pt\"><span style=\"font-family:Calibri,sans-serif\"><strong>Rabies vaccine</strong> -&nbsp;<strong>Killed &lsquo;inactivated&rsquo; vaccines</strong></span></span></p>\r\n\r\n<p>&nbsp;</p>\r\n\r\n<p><span style=\"font-size:12pt\"><span style=\"font-family:Calibri,sans-serif\"><strong><u>Explanation:</u></strong></span></span></p>\r\n\r\n<p>&nbsp;</p>\r\n\r\n<p><span style=\"font-size:12pt\"><span style=\"font-family:Calibri,sans-serif\"><strong>Option A. Sabin: </strong>&nbsp;<strong>Live attenuated Oral Polio Vaccine</strong></span></span></p>\r\n\r\n<p>&nbsp;</p>\r\n\r\n<p><span style=\"font-size:12pt\"><span style=\"font-family:Calibri,sans-serif\"><strong>Option B. Rota virus: Live attenuated oral vaccine&nbsp;</strong></span></span></p>\r\n\r\n<p>&nbsp;</p>\r\n\r\n<p><span style=\"font-size:12pt\"><span style=\"font-family:Calibri,sans-serif\"><strong>Option C. 17 D</strong> - <strong>Live attenuated</strong>, <strong>lyophilized</strong> (Freeze dried) <strong>Yellow fever vaccine&nbsp;</strong></span></span></p>\r\n\r\n<p>&nbsp;</p>\r\n\r\n<p><span style=\"font-size:12pt\"><span style=\"font-family:Calibri,sans-serif\"><strong><u>Educational Objective:</u></strong></span></span></p>\r\n\r\n<p>&nbsp;</p>\r\n\r\n<p style=\"text-align:justify\"><span style=\"font-size:16px\"><img src=\"https://cerebellum-web-static.s3.amazonaws.com/media/public/images/2023/08/23/screenshot-2023-08-11-1921039026_SBEBAIV.jpg\" style=\"height:386px; width:1000px\" /></span></p>\r\n\r\n<p style=\"text-align:justify\">&nbsp;</p>\r\n\r\n<p style=\"text-align:justify\"><span style=\"font-size:12pt\"><span style=\"font-family:Calibri,sans-serif\"><strong><u>Ref:</u></strong><strong> Park 26<sup>th</sup> ed pg 111.&nbsp;</strong></span></span></p>",
