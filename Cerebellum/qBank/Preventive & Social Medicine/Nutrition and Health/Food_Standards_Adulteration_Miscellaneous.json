      "text": "<p style=\"text-align:justify\"><span style=\"font-size:12pt\"><span style=\"font-family:&quot;Times New Roman&quot;,serif\"><span style=\"font-family:&quot;Calibri&quot;,sans-serif\">The following condition is caused by:</span></span></span></p>\r\n\r\n<p>&nbsp;</p>\r\n\r\n<p><span style=\"font-size:16px\"><span dir=\"ltr\" lang=\"EN-IN\"><span style=\"font-family:&quot;Times New Roman&quot;,serif\"><img alt=\"\" src=\"https://cerebellum-web-static.s3.amazonaws.com/media/public/images/2023/03/29/22.jpg\" style=\"height:370px; width:400px\" /></span></span></span></p>",
      "unique_key": "Q1403208",
      "question_audio": null,
      "question_video": null,
