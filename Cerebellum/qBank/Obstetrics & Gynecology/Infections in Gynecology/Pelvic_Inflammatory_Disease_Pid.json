        14
      ],
      "solution": "<p><span style=\"font-size:12pt\"><span style=\"font-family:&quot;Times New Roman&quot;,serif\"><strong><span style=\"font-family:&quot;Calibri&quot;,sans-serif\">Ans. A) Gray Kit (kit 1)</span></strong></span></span></p>\r\n\r\n<p>&nbsp;</p>\r\n\r\n<ul>\r\n\t<li><span style=\"font-size:12pt\"><span style=\"font-family:Calibri,sans-serif\"><span dir=\"ltr\" lang=\"EN-GB\">PID treatment includes treatment of both partners: kit 6 for woman and kit 1 for husband. Use of barrier contraceptive is advised. </span></span></span></li>\r\n\t<li><span style=\"font-size:12pt\"><span style=\"font-family:Calibri,sans-serif\">In syndromic approach</span></span>\r\n\t<ul>\r\n\t\t<li><span style=\"font-size:12pt\"><span style=\"font-family:Calibri,sans-serif\">Kit 1: gray: urethral discharge (men), cervical discharge (women)</span></span></li>\r\n\t\t<li><span style=\"font-size:12pt\"><span style=\"font-family:Calibri,sans-serif\">Kit 2: green: vaginal discharge</span></span></li>\r\n\t\t<li><span style=\"font-size:12pt\"><span style=\"font-family:Calibri,sans-serif\">Kit 6: yellow kit: PID/ lower abdominal pain syndrome</span></span></li>\r\n\t\t<li><span style=\"font-size:12pt\"><span style=\"font-family:Calibri,sans-serif\">Kit 3:&nbsp;white / 4 blue: non herpetic ulcers</span></span></li>\r\n\t\t<li><span style=\"font-size:12pt\"><span style=\"font-family:Calibri,sans-serif\">Kit 5: red: herpetic ulcer</span></span></li>\r\n\t\t<li><span style=\"font-size:12pt\"><span style=\"font-family:Calibri,sans-serif\">Kit 7: black: inguinal bubo</span></span></li>\r\n\t</ul>\r\n\t</li>\r\n</ul>\r\n\r\n<p>&nbsp;</p>\r\n\r\n<p><span style=\"font-size:12pt\"><span style=\"font-family:&quot;Times New Roman&quot;,serif\"><strong><span style=\"font-family:&quot;Calibri&quot;,sans-serif\">Educational Objective: </span></strong></span></span></p>\r\n\r\n<p>&nbsp;</p>\r\n\r\n<p><span style=\"font-size:12pt\"><span style=\"font-family:&quot;Times New Roman&quot;,serif\"><strong><span style=\"font-family:&quot;Calibri&quot;,sans-serif\"><img alt=\"\" src=\"https://cerebellum-web-static.s3.amazonaws.com/media/public/images/2024/01/08/picture12.jpg\" style=\"height:558px; width:800px\" /></span></strong></span></span></p>\r\n\r\n<p>&nbsp;</p>\r\n\r\n<p><span style=\"font-size:12pt\"><span style=\"font-family:&quot;Times New Roman&quot;,serif\"><strong><span style=\"font-family:&quot;Calibri&quot;,sans-serif\">Reference: National AIDS Control Organization (NACO) colour-coded kits for syndromic management of sexually transmitted infections (STIs)</span></strong></span></span></p>",
