        {
          "id": 48996,
          "text": "<p style=\"text-align:justify\"><span style=\"font-size:11pt\"><span style=\"font-family:Calibri,sans-serif\"><span dir=\"ltr\" lang=\"EN-US\" style=\"font-size:12.0pt\">Induction of labor with PGE2 gel</span></span></span></p>"
        },
        {
          "id": 48997,
          "text": "<p style=\"text-align:justify\"><span style=\"font-size:11pt\"><span style=\"font-family:Calibri,sans-serif\"><span dir=\"ltr\" lang=\"EN-US\" style=\"font-size:12.0pt\">Continue antepartum fetal surveillance till 42 weeks </span></span></span></p>"
