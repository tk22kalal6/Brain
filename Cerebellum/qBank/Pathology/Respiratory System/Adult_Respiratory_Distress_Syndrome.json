        {
          "id": 59597,
          "text": "<p><span style=\"font-size:12pt\"><span style=\"font-family:Aptos,sans-serif\"><span style=\"font-family:&quot;Calibri&quot;,sans-serif\">Aspiration</span></span></span></p>"
        },
        {
          "id": 59598,
          "text": "<p><span style=\"font-size:12pt\"><span style=\"font-family:Aptos,sans-serif\"><span style=\"font-family:&quot;Calibri&quot;,sans-serif\">Injury to alveolar capillary endothelium</span></span></span></p>"
        },
