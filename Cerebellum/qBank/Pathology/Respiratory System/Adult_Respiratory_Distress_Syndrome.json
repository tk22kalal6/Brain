      "id": 14909,
      "choices": [
        {
          "id": 59596,
          "text": "<p><span style=\"font-size:12pt\"><span style=\"font-family:Aptos,sans-serif\"><span style=\"font-family:&quot;Calibri&quot;,sans-serif\">Disseminated intravascular coagulation</span></span></span></p>"
